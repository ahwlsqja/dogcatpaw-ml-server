# 서버 설정
GRPC_PORT=50052
GRPC_WORKERS=10
GRPC_MAX_MESSAGE_LENGTH=10485760

# 모델 설정
MODEL_PATH=embedder_model.onnx
MODEL_INPUT_SIZE=96
MODEL_INPUT_CHANNELS=1

# 로깅
LOG_LEVEL=INFO
LOG_FORMAT=json
ENVIRONMENT=production

# NCP Object Storage 설정 (선택사항)
# NCP Object Storage를 사용하려면 아래 설정을 입력하세요.
# 설정하지 않으면 로컬 파일 경로(MODEL_PATH)를 사용합니다.

# NCP_ACCESS_KEY=your_access_key_here
# NCP_SECRET_KEY=your_secret_key_here
# NCP_ENDPOINT=https://kr.object.ncloudstorage.com
# NCP_REGION=kr-standard
# NCP_BUCKET_NAME=your_bucket_name
# NCP_MODEL_KEY=models/embedder_model.onnx

# 모델 캐시 디렉토리 (NCP에서 다운로드한 모델 저장 위치)
MODEL_CACHE_DIR=/tmp/models
